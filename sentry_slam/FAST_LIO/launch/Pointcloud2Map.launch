<launch>
    <node pkg="octomap_server" type = "octomap_server_node" name="octomap_server">

        <param name ="resolution" value="0.05" />
        <param name = "frame_id" type="str" value="map" />
        <param name = "sensor_model/max_range" value="100.0" />
        <param name = "latch" value="true" />
        <param name = "pointcloud_max_z" value="0.5" />
        <param name = "pointcloud_min_z" value="0.0" />
        <param name = "ground_filter/angle" value="5.0" />
        <param name = "ground_filter/plane_distance" value="1.0" />
        <param name = "filter_speckles" value="true" />

        <remap from ="cloud_in" to="/cloud_registered" />
    </node>
</launch>